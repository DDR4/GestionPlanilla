@using Microsoft.AspNet.Identity
@{
    ViewBag.Title = "Reportes";
}

<section class="content-header">
    <h1>Reportes</h1>
    <br />

</section>
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">&nbsp;</h3>
                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                            <i class="fa fa-minus"></i>
                        </button>
                    </div>
                </div>
                <div class="box-body ">
                    <div class="row">
                        <div class="form-group col-md-2">
                            <label>Fecha de Reporte:</label>
                        </div>
                        <div class="form-group col-md-2">
                            <input type="text" class="form-control input-sm" id="txtFechaReporte" readonly>
                        </div>
                        <div class="form-group col-md-2">
                            <button id="btnGenerarReporteTurno" class="btn btn-success btn-sm" type="button"><span class="fa fa-file-excel-o"></span> Generar Reporte Turno</button>
                        </div>
                        <div class="form-group col-md-2">
                            <button id="btnGenerarReporteArea" class="btn btn-success btn-sm" type="button"><span class="fa fa-file-excel-o"></span> Generar Reporte Area</button>
                        </div>
                    </div>
                </div>
                <br />
            </div>
        </div>
    </div>

</section>

@section scripts{
    <script src="~/Content/js/Reporte/reporte.js"></script>
}

